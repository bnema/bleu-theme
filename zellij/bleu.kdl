themes {
    bleu {
        // Text components - for general UI text elements
        text_unselected {
            base 224 236 244        // #e0ecf4 - Primary Text
            background 5 10 20      // #050a14 - Deep Navy background
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange (warnings)
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan (info)
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue (special)
        }

        text_selected {
            base 254 254 254        // #fefefe - Pure White (emphasis)
            background 45 74 107    // #2d4a6b - Ocean Blue (selection)
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        // Ribbon components - for tabs and status bars
        ribbon_unselected {
            base 112 128 144        // #708090 - Dimmed Text (inactive tabs)
            background 7 12 22      // #070c16 - Dark Blue (inactive tab bg)
            emphasis_0 161 103 165  // #A167A5 - Soft Purple (errors)
            emphasis_1 224 236 244  // #e0ecf4 - Primary Text
            emphasis_2 85 136 204   // #5588cc - Pure Blue
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        ribbon_selected {
            base 224 236 244        // #e0ecf4 - Primary Text
            background 15 21 32     // #0f1520 - Active Tab Blue
            emphasis_0 161 103 165  // #A167A5 - Soft Purple
            emphasis_1 253 189 133  // #FDBD85 - Peach Orange
            emphasis_2 0 212 255    // #00d4ff - Bright Cyan
            emphasis_3 85 136 204   // #5588cc - Pure Blue
        }

        // Table components - for session manager and other table views
        table_title {
            base 0 212 255          // #00d4ff - Bright Cyan (headers)
            background 5 10 20      // #050a14 - Deep Navy
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        table_cell_unselected {
            base 224 236 244        // #e0ecf4 - Primary Text
            background 5 10 20      // #050a14 - Deep Navy
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        table_cell_selected {
            base 254 254 254        // #fefefe - Pure White (emphasis)
            background 45 74 107    // #2d4a6b - Ocean Blue (selection)
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        // List components - for file browsers and selection lists
        list_unselected {
            base 224 236 244        // #e0ecf4 - Primary Text
            background 5 10 20      // #050a14 - Deep Navy
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        list_selected {
            base 254 254 254        // #fefefe - Pure White (emphasis)
            background 45 74 107    // #2d4a6b - Ocean Blue (selection)
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        // Frame components - for pane borders and dividers
        frame_unselected {
            base 26 35 50           // #1a2332 - Card Blue (unfocused borders, subtle but visible)
            background 5 10 20      // #050a14 - Deep Navy background
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        frame_selected {
            base 85 136 204         // #5588cc - Pure Blue (active pane border)
            background 5 10 20      // #050a14 - Deep Navy background
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        frame_highlight {
            base 45 74 107          // #2d4a6b - Ocean Blue (hover state border)
            background 5 10 20      // #050a14 - Deep Navy background
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        // Exit code components - for command completion status
        exit_code_success {
            base 153 255 228        // #99FFE4 - Success Green
            background 5 10 20      // #050a14 - Deep Navy
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        exit_code_error {
            base 161 103 165        // #A167A5 - Soft Purple (errors)
            background 5 10 20      // #050a14 - Deep Navy
            emphasis_0 253 189 133  // #FDBD85 - Peach Orange
            emphasis_1 0 212 255    // #00d4ff - Bright Cyan
            emphasis_2 153 255 228  // #99FFE4 - Success Green
            emphasis_3 135 206 235  // #87ceeb - Light Sky Blue
        }

        // Multiplayer user colors - for collaborative sessions
        multiplayer_user_colors {
            player_1 0 212 255      // #00d4ff - Bright Cyan
            player_2 85 136 204     // #5588cc - Pure Blue
            player_3 153 255 228    // #99FFE4 - Success Green
            player_4 253 189 133    // #FDBD85 - Peach Orange
            player_5 135 206 235    // #87ceeb - Light Sky Blue
            player_6 74 123 167     // #4a7ba7 - Sky Blue
            player_7 161 103 165    // #A167A5 - Soft Purple
            player_8 45 74 107      // #2d4a6b - Ocean Blue
            player_9 254 254 254    // #fefefe - Pure White
            player_10 224 236 244   // #e0ecf4 - Primary Text
        }

        // Terminal colors (ANSI) - standard terminal color palette
        black "#050a14"         // Deep Navy - Primary background
        red "#A167A5"           // Soft Purple - Errors
        green "#99FFE4"         // Success Green
        yellow "#FDBD85"        // Peach Orange - Warnings
        blue "#5588cc"          // Pure Blue
        magenta "#87ceeb"       // Light Sky Blue
        cyan "#00d4ff"          // Bright Cyan
        white "#e0ecf4"         // Primary Text
        orange "#FDBD85"        // Peach Orange

        // Bright terminal colors
        bright_black "#2d4a6b"   // Ocean Blue
        bright_red "#A167A5"     // Soft Purple - Errors
        bright_green "#99FFE4"   // Success Green
        bright_yellow "#FDBD85"  // Peach Orange
        bright_blue "#5588cc"    // Pure Blue
        bright_magenta "#87ceeb" // Light Sky Blue
        bright_cyan "#00d4ff"    // Bright Cyan
        bright_white "#fefefe"   // Pure White - Emphasis
    }
}
